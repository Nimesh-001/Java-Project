CREATE TABLE Attendance
(
	Student_id CHAR(12),
	Course_code CHAR(7),
	Session_Type CHAR(4),
	Status CHAR(8),
	Session_Date DATE,
	Department_id CHAR(9),


	//PRIMARY KEY(Student_id,course_code,Session_Type,Session_Date),


	FOREIGN KEY (Course_Code) REFERENCES CourseUnit(course_code),
	FOREIGN KEY (student_id) REFERENCES Student(Student_id),
	FOREIGN KEY(Department_id) REFERENCES Department(Department_id)
);
