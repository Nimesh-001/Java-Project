CREATE TABLE Medical
(
	Medical_id CHAR(7),
	Student_id CHAR(12),
	Course_code CHAR(7),
	Session_Id CHAR(7),
	Submission_Date DATE,
	Exam_or_Lecture VARCHAR(15),
	Medical_description VARCHAR(20),
	Start_date DATE,
	End_Date DATE,
	Reason VARCHAR(100),
	Department_id CHAR(9),
	PRIMARY KEY(Medical_id,course_code),
	FOREIGN KEY (Course_Code) REFERENCES CourseUnit(course_code),
	FOREIGN KEY (student_id) REFERENCES Student(Student_id),
	FOREIGN KEY(Department_id) REFERENCES Department(Department_id)
);
